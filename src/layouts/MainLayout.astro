---
import BaseHead from "@/components/BaseHead.astro";
import HeaderHome from "@/components/HeaderHome.astro";
interface Props {
    title: string;
}
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
    <BaseHead title={title} />
    <HeaderHome />
    <body class="bg-red-50 select-none">
    
        <slot />
    </body>
</html>
